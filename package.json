		"react-qr-svg": "^2.2.1",
		"react-redux": "^6.0.1",
		"react-responsive": "^6.1.1",
		"react-router-dom": "^5.0.0",
		"react-scripts": "2.1.8",
		"redux": "^4.0.1",
		"redux-saga": "^1.0.2",
		"request": "^2.88.0",
		"request-promise": "^4.2.4",
